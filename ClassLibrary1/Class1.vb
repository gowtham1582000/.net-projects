Public Class Class1
    Public Function add(n1 As Int16, n2 As Int16)
        Return n1 + n2
    End Function
    Public Function subtraction(n1 As Int16, n2 As Int16)
        Return n1 - n2
    End Function
    Public Function multiply(n1 As Int16, n2 As Int16)
        Return n1 * n2
    End Function
    Public Function Division(n1 As Int16, n2 As Int16)
        Return n1 \ n2
    End Function
End Class
